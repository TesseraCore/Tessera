{
  "name": "@tessera/devtools",
  "version": "0.0.1",
  "description": "Development tools for debugging Tessera viewer",
  "type": "module",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "tessera": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.8.3"
  }
}

